{
  "name": "jupyterlab-scales",
  "version": "0.2.0",
  "description": "A JupyterLab extension for ipyscales. A widget library for scales",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.5",
    "@phosphor/coreutils": "~1.3.0",
    "jupyter-scales": "^1.1.0"
  },
  "devDependencies": {
    "@phosphor/application": "^1.5.0",
    "@phosphor/widgets": "^1.5.0",
    "rimraf": "^2.6.2",
    "typescript": "~2.6.2"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension",
    "jupyterlab notebook"
